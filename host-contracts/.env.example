# Example environment file for deploying FHEVM host contracts.
# Copy this file to `.env` and replace placeholder values before running scripts.

# RPC endpoint of the host chain
FHEVM_HOST_RPC_URL=https://localhost:8545

# Chain ID of the host chain
FHEVM_HOST_CHAIN_ID=1337

# Private key of the account that will deploy the host contracts
# Do NOT commit your real private key to version control.
FHEVM_HOST_DEPLOYER_KEY=0xYOUR_PRIVATE_KEY_HERE

# Optional: address that will be used for admin / governance actions
FHEVM_HOST_ADMIN_ADDRESS=0x0000000000000000000000000000000000000000

# Optional: custom gas overrides for deployment scripts
FHEVM_HOST_GAS_PRICE_GWEI=2
FHEVM_HOST_GAS_LIMIT=6000000
